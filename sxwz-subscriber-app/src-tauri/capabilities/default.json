{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "log:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://passport.bilibili.com"
        },
        {
          "url": "https://api.bilibili.com"
        }
      ]
    }
  ]
}